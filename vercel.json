{
  "version": 2,
  "builds": [
    { "src": "api/fix.js", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/fix", "dest": "/api/fix.js" },
    { "src": "/fixed/(.*)", "dest": "/public/fixed/$1" },
    { "src": "/", "dest": "/public/index.html" }
  ]
}
